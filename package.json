{
  "name": "buble-jest",
  "version": "1.0.0",
  "description":
    "Jest plugin to use buble and babel(handle ES Modules) for transformation",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "keywords": ["buble", "jest", "transform"],
  "author": "qingwei-li <cinwell.li@gmail.com> (https://github.com/QingWei-Li)",
  "repository": "qingwei-li/buble-jest",
  "homepage": "https://npm.im/buble-jest",
  "license": "MIT",
  "peerDependencies": {
    "buble": "*"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "./index.js"
    }
  },
  "devDependencies": {
    "buble": "^0.19.3",
    "jest": "^22.4.3"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.42",
    "cosmiconfig": "^4.0.0"
  },
  "engines": {
    "node": ">=4"
  }
}
